header.page-header
  // RD Navbar
  .rd-navbar-wrap
    nav.rd-navbar.rd-navbar_corporate(data-layout='rd-navbar-fixed', data-sm-layout='rd-navbar-fixed', data-sm-device-layout='rd-navbar-fixed', data-md-layout='rd-navbar-static', data-md-device-layout='rd-navbar-fixed', data-lg-device-layout='rd-navbar-static', data-lg-layout='rd-navbar-static', data-stick-up-clone='false', data-sm-stick-up='true' data-md-stick-up='true' data-lg-stick-up='true', data-md-stick-up-offset='145px', data-lg-stick-up-offset='145px', data-body-class='rd-navbar-corporate-linked')
      // RD Navbar Panel
      .rd-navbar-panel
        button.rd-navbar-toggle(data-rd-navbar-toggle='.rd-navbar-nav-wrap'): span

        // RD Navbar Brand
        .rd-navbar-brand.rd-navbar-static--hidden
          a.brand-name(href='/')
            +png(logoDefault)

      // RD Navbar Top Panel
      .rd-navbar-top-panel.rd-navbar-top-panel_extended: .rd-navbar-top-panel__main
        .rd-navbar-top-panel__toggle.rd-navbar-fixed__element-1.rd-navbar-static--hidden(data-rd-navbar-toggle='.rd-navbar-top-panel__main'): span
        .rd-navbar-top-panel__content
          //.rd-navbar-top-panel__content-top
            .rd-navbar-top-panel__left: p Premium HTML Template
            .rd-navbar-top-panel__right: ul.rd-navbar-items-list
              li: ul.list-inline-xxs
                li: a(href='#', data-toggle='modal', data-target='#modalLogin') Sign In
                li: a(href='#', data-toggle='modal', data-target='#modalRegister') Create an Account

              li: ul.list-inline-xxs
                li: a.icon.icon-xxs.icon-primary.fa.fa-facebook(href='#')
                li: a.icon.icon-xxs.icon-primary.fa.fa-twitter(href='#')
                li: a.icon.icon-xxs.icon-primary.fa.fa-google-plus(href='#')
                li: a.icon.icon-xxs.icon-primary.fa.fa-vimeo(href='#')
                li: a.icon.icon-xxs.icon-primary.fa.fa-youtube(href='#')
                li: a.icon.icon-xxs.icon-primary.fa.fa-pinterest-p(href='#')

          .rd-navbar-top-panel__content-bottom.rd-navbar-content_middle.rd-navbar-content_justify
            // RD Navbar Brand
            .rd-navbar-brand.rd-navbar-fixed--hidden
              a.brand-name(href='index.html')
                +png(logoDefault)

            ul.list-bordered
              li: .unit.unit-horizontal.unit-middle.unit-spacing-sm
                .unit__left: span.icon.icon-md.icon-primary.linear-icon-map-marker
                .unit__body: dl.list-terms-modern
                  dt Address
                  dd: a(href='#') 4578 Marmora Road, Glasgow, D04 89GR

              li: .unit.unit-horizontal.unit-middle.unit-spacing-sm
                .unit__left: span.icon.icon-md.icon-primary.linear-icon-telephone
                .unit__body: dl.list-terms-modern
                  dt Phones
                  dd: ul.list-semicolon
                    li: a(href='callto:#') (244) 80-4655
                    li: a(href='callto:#') (800) 123-0045
              li: .unit.unit-horizontal.unit-middle.unit-spacing-sm
                .unit__left: span.icon.icon-md.icon-primary.linear-icon-user
                .unit__body: dl.list-terms-modern
                  dd: a(href='#', data-toggle='modal', data-target='#modalLogin') Sign In
                  dd: a(href='#', data-toggle='modal', data-target='#modalRegister') Register
              //li: .unit.unit-horizontal.unit-middle.unit-spacing-sm
                .unit__left: span.icon.icon-md.icon-primary.linear-icon-clock3
                .unit__body: dl.list-terms-modern
                  dt We are open
                  dd Mn-Fr: 10 am-8 pm


      .rd-navbar-bottom-panel.rd-navbar-search-wrap
        // RD Navbar Nav
        .rd-navbar-nav-wrap.rd-navbar-search_not-collapsable
          ul.rd-navbar-items-list.rd-navbar-search_collapsable
            li: button.rd-navbar-search__toggle.rd-navbar-fixed--hidden(data-rd-navbar-toggle='.rd-navbar-search-wrap')
            li.rd-navbar-nav-wrap__shop: a.icon.icon-md.linear-icon-cart.link-gray-4(href='cart.html')

          include ../includes/_navbar-search
          .rd-navbar-search_collapsable
            include ../includes/_nav