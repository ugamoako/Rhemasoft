  .rd-navbar-wrap
    nav.rd-navbar(data-layout='rd-navbar-fixed', data-sm-layout='rd-navbar-fixed', data-sm-device-layout='rd-navbar-fixed', data-md-layout='rd-navbar-sidebar', data-md-device-layout='rd-navbar-fixed', data-lg-device-layout='rd-navbar-sidebar', data-lg-layout='rd-navbar-sidebar', data-stick-up-clone='false', data-sm-stick-up='true' data-md-stick-up='true' data-lg-stick-up='true', data-md-stick-up-offset='50px', data-lg-stick-up-offset='50px', data-body-class='rd-navbar-absolute', data-md-hover-on='false', data-lg-hover-on='false', data-sm-auto-height='false')
      include ../includes/_navbar-default-top-panel

      // RD Navbar Panel
      .rd-navbar-panel
